        <Border Margin="537,494,0,0"  Background="#AAADC4" BorderThickness="0" BorderBrush="#AAADC4"  Height="53"  CornerRadius="30"  Width="400" HorizontalAlignment="Left" VerticalAlignment="Top">
            <TextBlock Margin="0,15,0,0" Foreground="White"  Background="Transparent" HorizontalAlignment="Center" TextAlignment="Center"  Height="53"  TextWrapping="Wrap" Text="Device Type"  Width="400"  VerticalAlignment="Center" FontFamily="Leelawadee UI Semilight" FontSize="24"/>
        </Border>




        <Border Margin="345,282,0,0"  Background="#AAADC4"  Height="53"  CornerRadius="30"  Width="181" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Label Foreground="White" Content="Facebook"  Margin="0,0,0,0" HorizontalAlignment="Center"     VerticalAlignment="Center" FontFamily="Leelawadee UI Semilight" FontSize="24"/>
        </Border>
        <Border Margin="345,351,0,0"  Background="#AAADC4"  Height="53"  CornerRadius="30"  Width="181" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Label Foreground="White" Content="Active Status"  Margin="0,0,0,0" HorizontalAlignment="Center"     VerticalAlignment="Center" FontFamily="Leelawadee UI Semilight" FontSize="24"/>
        </Border>
        <Border Margin="345,424,0,0"  Background="#AAADC4"  Height="53"  CornerRadius="30"  Width="181" HorizontalAlignment="Left" VerticalAlignment="Top">
